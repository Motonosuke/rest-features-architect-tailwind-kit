    "markuplint": "^3.10.0",
    "nookies": "2.5.2",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "swr": "2.0.3",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "zod": "3.14.3"
