  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "lint": "eslint --ext .js,.ts,.tsx 'src/**/*.{js,ts,tsx}'",
