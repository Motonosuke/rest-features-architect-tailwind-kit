    "@markuplint/jsx-parser": "3.7.0",
    "@markuplint/react-spec": "3.8.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@typescript-eslint/eslint-plugin": "5.50.0",
